<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet author="NikolaM" id="1">
        <createTable tableName="mechanical_danger">
            <column name="danger_type" type="VARCHAR(255)"/>
            <column name="danger_number" type="INT"/>
            <column name="description" type="LONGTEXT"></column>
            <column name="id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet author="NikolaM" id="2">
        <addPrimaryKey columnNames="id" constraintName="mechanical_dangerPK" tableName="mechanical_danger"/>
    </changeSet>

</databaseChangeLog>