<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet author="NikolaM" id="1">
        <createTable tableName="mechanical_danger_details">
            <column name="value" type="DOUBLE"/>
            <column name="description" type="LONGTEXT"></column>
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints primaryKey="true" primaryKeyName="mechanical_danger_detailsPK" />
            </column>
            <column name="probability_id" type="BIGINT"></column>
            <column name="effect_id" type="BIGINT"></column>
            <column name="frequency_id" type="BIGINT"></column>
            <column name="mechanical_danger_id" type="BIGINT"></column>
        </createTable>
    </changeSet>
    <changeSet author="NikolaM" id="2">
        <addForeignKeyConstraint baseTableName="mechanical_danger_details" baseColumnNames="probability_id" constraintName="mechanical_danger_details_probabilityFK" referencedTableName="probability" referencedColumnNames="id" />
        <addForeignKeyConstraint baseTableName="mechanical_danger_details" baseColumnNames="effect_id" constraintName="mechanical_danger_details_effectFK" referencedTableName="effect" referencedColumnNames="id" />
        <addForeignKeyConstraint baseTableName="mechanical_danger_details" baseColumnNames="frequency_id" constraintName="mechanical_danger_details_frequencyFK" referencedTableName="frequency" referencedColumnNames="id" />
        <addForeignKeyConstraint baseTableName="mechanical_danger_details" baseColumnNames="mechanical_danger_id" constraintName="mechanical_danger_details_mechdangFK" referencedTableName="mechanical_danger" referencedColumnNames="id" />
    </changeSet>

</databaseChangeLog>